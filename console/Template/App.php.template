<?phpnamespace {{NAMESPACE}};use MonoKit\App\App;use MonoKit\Component\Service\MysqliService;Class {{NAME}} extends App{    protected $HOST_DEV = "127.0.0.1";    protected $DATABASE_DEV = "";    protected $LOGIN_DEV = "root";    protected $PASSWORD_DEV = "";    protected $HOST_PROD = "localhost";    protected $DATABASE_PROD = "";    protected $LOGIN_PROD = "";    protected $PASSWORD_PROD = "";    public function __construct( $mode = self::MODE_DEV )    {        parent::__construct( $mode );        if ( $this->isMode( self::MODE_DEV ) ) {            // DEV            $this->addService( new MysqliService( $this->HOST_DEV , $this->LOGIN_DEV , $this->PASSWORD_DEV , $this->DATABASE_DEV ) ) ;            $this->getAppRegistry()->set( "APPLICATION.ASSETS" , "" );        } else {            // PRODUCTION            $this->addService( new MysqliService( $this->HOST_PROD , $this->LOGIN_PROD , $this->PASSWORD_PROD , $this->DATABASE_PROD ) ) ;            $this->getAppRegistry()->set( "APPLICATION.ASSETS" , "" );        }    }}